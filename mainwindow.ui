QHeaderView::section {
    border: 1px solid #ccc;
}

QHeaderView::section:horizontal {
    background-color: #8b0000;
}

QHeaderView::section:vertical {
    background-color: #fff;
    color: #000;
}

/* Kiểu mặc định của thanh cuộn */
QScrollBar:vertical {
    border: none;
    background: #f0f0f0; /* Màu nền nhạt */
    width: 8px; /* Thanh cuộn nhỏ */
    margin: 0px 0px 0px 0px;
	border-radius: 4px;
}

QScrollBar::handle:vertical {
    background: #a0a0a0; /* Màu tay cầm nhạt */
    min-height: 40px;
    border-radius: 4px;
    transition: all 0.3s; /* Hiệu ứng chuyển đổi mượt mà */
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0px; /* Ẩn nút mũi tên */
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

/* Kiểu khi hover vào thanh cuộn */
QScrollBar:vertical:hover {
    width: 12px; /* Tăng kích thước khi hover */
}

QScrollBar::handle:vertical:hover {
    background: #808080; /* Thay đổi màu để dễ nhìn hơn khi hover */
    width: 12px; /* Đảm bảo kích thước tăng theo thanh cuộn */
}

QScrollBar::add-line:vertical:hover, QScrollBar::sub-line:vertical:hover {
    height: 12px; /* Hiển thị lại nút mũi tên khi hover */
}

/* Kiểu mặc định của thanh cuộn ngang */
QScrollBar:horizontal {
    border: none;
    background: #f0f0f0;
    height: 8px;
    margin: 0px 0px 0px 0px;
	border-radius: 4px;
}

QScrollBar::handle:horizontal {
    background: #a0a0a0;
    min-width: 6px;
    border-radius: 4px;
    transition: all 0.3s;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    border: none;
    background: none;
    width: 0px;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

/* Kiểu khi hover vào thanh cuộn ngang */
QScrollBar:horizontal:hover {
    height: 12px;
}

QScrollBar::handle:horizontal:hover {
    background: #808080;
    height: 12px;
}

QScrollBar::add-line:horizontal:hover, QScrollBar::sub-line:horizontal:hover {
    width: 12px;
}
